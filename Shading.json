        "id": "e341e34f.10384",
        "type": "function",
        "z": "238dca10.02b2b6",
        "name": "Schutzstatus prÃ¼fen",
        "func": "if (msg.Beschattung.RainProtection === true) {\n    msg.Beschattung.Status = \"Regenschutz\";\n    return [ msg, null, null ];\n} else if (msg.Beschattung.SunProtection === true) {\n    msg.Beschattung.Status = \"Sonnenschutz\";\n    return [ null, msg, null ];\n} else {\n    msg.Beschattung.Status = \"Kein Schutz nÃ¶tig\";\n    return [ null, null, msg ];\n}",
